    "import random\n",
    "seq=list(range(1, 20))\n",
    "n_group=4\n",
    "n=5\n",
    "group=[]\n",
    "random.shuffle(seq)\n",
    "for i in range(n_group):\n",
    "    temp=seq[i*n : (i+1)*n]\n",
    "    group.append(temp)"
